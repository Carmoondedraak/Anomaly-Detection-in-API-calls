  "paths": {
    "/v2/oauth2/token": {
      "post": {
        "tags": [
          "token"
        ],
        "summary": "Partner makes a request to the token endpoint by adding the following parameters describerd  below",
        "description": "Multiple status values can be provided with comma separated strings",
        "requestBody": {
          "content": {
            "application/x-www-form-urlencoded": {
              "schema": {
                "required": [
                  "grant_type"
                ],
                "properties": {
                  "grant_type": {
                    "type": "string",
                    "description": "Status values that need to be considered for filter",
                    "enum": [
                      "client_credentials"
                    ]
                  }
                }
              }
            },
            "application/json": {
              "schema": {
                "required": [
                  "grant_type"
                ],
                "properties": {
                  "grant_type": {
                    "type": "string",
                    "description": "Status values that need to be considered for filter",
                    "enum": [
                      "client_credentials"
                    ]
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Authorisation token (Bearer)",
            "content": {
              "application/x-www-form-urlencoded": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "scope": {
                      "type": "string",
                      "description": "The access token value"
                    },
                    "access_token": {
                      "type": "string",
                      "description": "The access token value"
                    },
                    "token_type": {
                      "type": "string",
                      "description": "Type of the token is set to Bearer",
                      "enum": [
                        "Bearer"
                      ]
                    },
                    "app_id": {
                      "type": "string",
                      "description": "The access token value"
                    },
                    "expires_in": {
                      "type": "integer",
                      "description": "The lifetime in seconds of the access token",
                      "format": "int32"
                    },
                    "nonce": {
                      "type": "string",
                      "description": "The access token value"
                    }
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "scope": {
                      "type": "string",
                      "description": "The access token value"
                    },
                    "access_token": {
                      "type": "string",
                      "description": "The access token value"
                    },
                    "token_type": {
                      "type": "string",
                      "description": "Type of the token is set to Bearer",
                      "enum": [
                        "Bearer"
                      ]
                    },
                    "app_id": {
                      "type": "string",
                      "description": "The access token value"
                    },
                    "expires_in": {
                      "type": "integer",
                      "description": "The lifetime in seconds of the access token",
                      "format": "int32"
                    },
                    "nonce": {
                      "type": "string",
                      "description": "The access token value"
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/x-www-form-urlencoded": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    },
                    "error_description": {
                      "type": "string"
                    }
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    },
                    "error_description": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/x-www-form-urlencoded": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    },
                    "error_description": {
                      "type": "string"
                    }
                  }
                }
              },
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    },
                    "error_description": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "BasicAuth": []
          }
        ]
      }
    },
    "/v2/checkout/orders": {
      "post": {
        "tags": [
          "order"
        ],
        "summary": "Checks out and places order",
        "description": "Authorise Payment for placed order",
        "requestBody": {
          "description": "Intent Capture",
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "properties": {
                  "intent": {
                    "type": "string"
                  },
                  "purchase_units": {
                    "type": "array",
                    "items": {
                      "$ref": "#/components/schemas/models.Amount"
                    }
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "id": {
                      "type": "string"
                    },
                    "status": {
                      "type": "string"
                    },
                    "links": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/models.Link"
                      }
                    }
                  }
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "message": {
                      "type": "string"
                    },
                    "debug_id": {
                      "type": "string"
                    },
                    "details": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/models.Detail"
                      }
                    },
                    "links": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/models.Link"
                      }
                    }
                  }
                }
              }
            }
          },
          "401": {
            "description": "Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "error": {
                      "type": "string"
                    },
                    "error_description": {
                      "type": "string"
                    }
                  }
                }
              }
            }
          },
          "403": {
            "description": "Forbidden",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string"
                    },
                    "message": {
                      "type": "string"
                    },
                    "debug_id": {
                      "type": "string"
                    },
                    "details": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/models.Detail"
                      }
                    },
                    "links": {
                      "type": "array",
                      "items": {
                        "$ref": "#/components/schemas/models.Link"
                      }
                    }
                  }
                }
              }
            }
          }
        },
        "security": [
          {
            "Bearer": []
          }
        ],
        "x-codegen-request-body-name": "payment"
      }
    },
